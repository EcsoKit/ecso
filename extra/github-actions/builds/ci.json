{
	"template": "ci",
	"defaults": {
		"development": true,
		"haxeVersion": "4.2.4",
		"url": "https://raw.githubusercontent.com/HaxeFoundation/haxe/2a9ef7cf837d172351b62921ff6695f7d3a172a4/.github/workflows/main.yml",
		"haxeSources": "HaxeFoundation/haxe@ab0c0548ff80fcbbbc140a381a9031af13b5782c",
		"experimentalTargets": ["jvm"]
	},
	"workflows": [
		{
			"name": "win64",
			"nekoDownload": "https://build.haxe.org/builds/neko/windows64/neko_latest.zip",
			"jobs": ["windows64-build", "windows64-test"],
			"os": {
				"name": "windows",
				"version": "latest"
			}
		},
		{
			"name": "win32",
			"nekoDownload": "https://build.haxe.org/builds/neko/windows/neko_2019-12-02_master_1df580c.zip",
			"jobs": ["windows-build", "windows-test"],
			"os": {
				"name": "windows",
				"version": "latest"
			},
			"allowFailure": true
		},
		{
			"name": "linux64",
			"nekoDownload": "https://build.haxe.org/builds/neko/linux64/neko_2019-12-02_master_1df580c.tar.gz",
			"jobs": ["linux-build", "linux-test"],
			"os": {
				"name": "ubuntu",
				"version": "latest"
			}
		},
		{
			"name": "mac",
			"nekoDownload": "https://build.haxe.org/builds/neko/mac/neko_2019-12-02_master_1df580c.tar.gz",
			"jobs": ["mac-build", "mac-test"],
			"os": {
				"name": "macos",
				"version": "latest"
			}
		}
	]
}